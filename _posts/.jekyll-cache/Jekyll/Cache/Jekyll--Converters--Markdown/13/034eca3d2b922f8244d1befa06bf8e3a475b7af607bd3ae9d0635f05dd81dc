<span class="n">ì´ë¦„</span> <span class="o">=</span> <span class="p">[(</span><span class="n">í—¤ë”</span><span class="p">,[</span><span class="n">ë°ì´í„°</span> <span class="n">ë¦¬ìŠ¤íŠ¸</span><span class="p">]),</span> <span class="p">(</span><span class="n">í—¤ë”2</span><span class="p">,</span> <span class="p">[</span><span class="n">ë°ì´í„°</span> <span class="n">ë¦¬ìŠ¤íŠ¸2</span><span class="p">])]</span>

<span class="n">df</span> <span class="o">=</span> <span class="n">pd</span><span class="p">.</span><span class="n">DataFrame</span><span class="p">.</span><span class="n">from_items</span><span class="p">(</span><span class="n">ì´ë¦„</span><span class="p">)</span>

<span class="kn">import</span> <span class="nn">pandas</span> <span class="k">as</span> <span class="n">pd</span>
 
<span class="n">friend_list</span> <span class="o">=</span> <span class="p">[</span>
    <span class="p">[</span><span class="s">'name'</span><span class="p">,</span> <span class="p">[</span><span class="s">'John'</span><span class="p">,</span> <span class="s">'Nate'</span><span class="p">]],</span>
    <span class="p">[</span><span class="s">'age'</span><span class="p">,</span> <span class="p">[</span><span class="mi">20</span><span class="p">,</span><span class="mi">30</span><span class="p">]],</span>
    <span class="p">[</span><span class="s">'job'</span><span class="p">,</span> <span class="p">[</span><span class="s">'student'</span><span class="p">,</span><span class="s">'teacher'</span><span class="p">]]</span>
<span class="p">]</span>

